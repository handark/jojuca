                           <div class="container">
                              <div class="row">
                                 
                                       <div class="col-md-8 col-md-offset-2">
                                          
                                             <!-- Heading -->
                                             <div class="heading center">
                                                <h2>Crea una Cuenta gratis</h2>
                                                <p class="subheadline">Registrate de forma gratuita y empieza a vivir la experiencia de usar Jojuca</p>
                                             </div>
                                             <!-- Heading End -->                                           
                                       
                                       </div>
                                       
                                       <div class="col-md-6 col-md-offset-3" ng-controller="RegistroUsuariosCtrl">

                                             <div class="alert alert-success alert-dark" ng-show="registro.registrado">
                                                <strong>Registro exitoso!</strong> Hemos enviado un email para activar su cuenta.
                                             </div>

                                             <div class="alert alert-danger" ng-show="registro.errores">
                                                <button type="button" class="close" data-dismiss="alert">×</button>
                                                {{registro.errorDescripcion}}
                                             </div>
                                                  
                                             <form role="form"  name="formRegistro" id="formRegistro" class="clearfix">
                                                <input type="text" name="nombre" required ng-model="registro.nombre" id="nombre" class="input-field form-control" placeholder="Nombre">
                                                <input type="text" name="email" required id="email" ng-model="registro.email" class="input-field form-control" placeholder="Correo Electronico">
                                                <input type="password" required name="contrasena" ng-model="registro.contrasena" id="contrasena" class="input-field form-control" placeholder="Contraseña">
                                                <input type="password" required name="verificaContrasena" ng-model="registro.verificaContrasena" id="verificaContrasena" class="input-field form-control" placeholder="Verifica la Contraseña">
                                                <input type="submit" value="Aceptar" name="submit" class="submit" id="submit">
                                             </form>

                                             <div>
                                                <a href="#">¿Ya tienes una cuenta?</a>
                                             </div>
                                             <!-- Contact Form End --> 

                                       </div>
                              
