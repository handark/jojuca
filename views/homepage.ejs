<!-- Contact Section -->
<section id="contact" class="clearfix" data-scroll-index="5">
   
      <!-- Contact -->
      <div class="contact">
         
            <div class="container">
               <div class="row">
                  
                        <div class="col-md-8 col-md-offset-2">
                           
                              <!-- Heading -->
                              <div class="heading center">
                                 <h2>Crea una Cuenta gratis</h2>
                                 <p class="subheadline">Registrate de forma gratuita y empieza a vivir la experiencia de usar Jojuca</p>
                              </div>
                              <!-- Heading End -->                                           
                        
                        </div>
                        
                        <div class="col-md-6 col-md-offset-3" ng-controller="RegistroUsuariosCtrl">

                              <div class="alert alert-success alert-dark" ng-show="registro.registrado">
                                 <strong>Registro exitoso!</strong> Hemos enviado un email para activar su cuenta.
                              </div>

                              <div class="alert alert-danger" ng-show="registro.errores">
                                 <button type="button" class="close" data-dismiss="alert">×</button>
                                 {{registro.errorDescripcion}}
                              </div>
                                   
                              <form role="form"  name="formRegistro" id="formRegistro" class="clearfix">
                                 <input type="text" name="nombre" required ng-model="registro.nombre" id="nombre" class="input-field form-control" placeholder="Nombre">
                                 <input type="text" name="email" required id="email" ng-model="registro.email" class="input-field form-control" placeholder="Correo Electronico">
                                 <input type="password" required name="contrasena" ng-model="registro.contrasena" id="contrasena" class="input-field form-control" placeholder="Contraseña">
                                 <input type="password" required name="verificaContrasena" ng-model="registro.verificaContrasena" id="verificaContrasena" class="input-field form-control" placeholder="Verifica la Contraseña">
                                 <input type="submit" value="Aceptar" name="submit" class="submit" id="submit">
                              </form>

                              <div>
                                 <a href="#" data-toggle="modal" data-target="#modalIngresar" >¿Ya tienes una cuenta?</a>
                              </div>
                              <!-- Contact Form End --> 

                        </div>
               
               </div><!-- .row End -->
            </div><!-- .container End -->
      
      </div>
      <!-- Contact End -->

</section>
<!-- Contact Section End -->

<!-- Modal Ingreasar a Jojuca -->
<div class="modal fade" id="modalIngresar" tabindex="-1" role="dialog" aria-labelledby="modalIngresarLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title portal-titulo-modal text-center" id="modalIngresarLabel">Ingresar a Jojuca</h4>
      </div>
      <div class="modal-body" ng-controller="IngresoUsuariosCtrl">

         <p class="subtituloModal text-center">Ingresa tus datos para acceder a Jojuca</p>

        <div class="alert alert-danger" ng-show="ingreso.errores">
            <button type="button" class="close" data-dismiss="alert">×</button>
            {{ingreso.errorDescripcion}}
         </div>
              
         <form role="form"  name="formIngreso" id="formIngreso" class="clearfix">
            <input type="text" name="ingresoEmail" ui-keyup="{'enter':'submitFormIngreso($event)'}" required id="ingresoEmail" ng-model="ingreso.email" class="input-field form-control input-portal" placeholder="Correo Electronico">
            <input type="password" ui-keyup="{'enter':'submitFormIngreso($event)'}" required name="ingresoContrasena" ng-model="ingreso.contrasena" id="ingresoContrasena" class="input-field form-control input-portal" placeholder="Contraseña">
         </form>
         <div>
            <a href="#" data-toggle="modal" data-target="#modalRecordarContrasena" >¿Olvidades tu Contraseña?</a>
         </div>
      </div>
      <div class="modal-footer">
          <button type="button" class="btn btn-portal" onclick="$('#formIngreso').submit();" >Ingresar</button>
      </div>
    </div>
  </div>
</div>   

<!-- Modal Recordar Contraseña -->
<div class="modal fade" id="modalRecordarContrasena" tabindex="-1" role="dialog" aria-labelledby="modalRecordarContrasenaLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title portal-titulo-modal text-center" id="modalRecordarContrasenaLabel">Recordar Contraseña</h4>
      </div>
      <div class="modal-body" ng-controller="RecordarContrasenaCtrl">
         
         <p class="subtituloModal text-center">Ingresa tu Correo Electronico para enviarte un enlace con las instrucciones para reestablecer la contraseña</p>

         <form role="form"  name="formRecordarContrasena" id="formRecordarContrasena" class="clearfix">
            <input type="text" name="recordarContrasenaEmail" required id="recordarContrasenaEmail" ng-model="recordarContrasena.email" class="input-field form-control input-portal" ui-keyup="{'enter':'submitFormRecordarContrasena($event)'}"  placeholder="Ingrese su Correo Electronico">
         </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-portal" onclick="$('#formRecordarContrasena').submit();"  >Recordar</button>
      </div>
    </div>
  </div>
</div>               